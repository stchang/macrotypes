(define (scd-ref [n : Int] [scd : (SimpleCatDeque A)] → (Option A))
  (if (< n 1)
    (scd-head scd)
    (match (scd-tail scd) with
     [None -> None]
     [Some tl -> (scd-ref (- n 1) tl)])))

